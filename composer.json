{
    "name": "digitonic/laravel-facebook-sdk",
    "description": "Fully unit tested Facebook SDK v5 integration for Laravel & Lumen 5.x",
    "keywords": ["facebook", "laravel", "lumen", "sdk", "facebook sdk v4", "facebook sdk v5"],
    "homepage": "https://github.com/digitonic/LaravelFacebookSdk",
    "license": "MIT",
    "authors": [
        {
            "name": "Sammy Kaye Powers",
            "email": "me@sammyk.me",
            "homepage": "http://sammyk.me",
            "role": "Developer"
        },
        {
            "name": "Steven Richardson",
            "email": "steven@digitonic.co.uk",
            "homepage": "https://digitonic.co.uk",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.3.0",
        "illuminate/auth": "^6.0|^7.0|^8.0",
        "illuminate/config": "^6.0|^7.0|^8.0",
        "illuminate/database": "^6.0|^7.0|^8.0",
        "illuminate/routing": "^6.0|^7.0|^8.0",
        "illuminate/session": "^6.0|^7.0|^8.0",
        "illuminate/support": "^6.0|^7.0|^8.0",
        "facebook/graph-sdk": "^5.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.3.3",
        "mockery/mockery": "~1.0",
        "squizlabs/php_codesniffer": "~2.0"
    },
    "autoload": {
        "psr-4": {
            "SammyK\\LaravelFacebookSdk\\": "src/LaravelFacebookSdk"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SammyK\\LaravelFacebookSdk\\Test\\": "tests"
        }
    },
    "branch-alias": {
        "dev-master": "4.x-dev"
    },
    "extra": {
        "laravel": {
            "providers": [
                "SammyK\\LaravelFacebookSdk\\LaravelFacebookSdkServiceProvider"
            ],
            "aliases": {
                "Facebook": "SammyK\\LaravelFacebookSdk\\FacebookFacade"
            }
        }
    }
}
